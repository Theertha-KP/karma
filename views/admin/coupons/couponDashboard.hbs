    <!-- ratio js -->
    <script src="/assets/js/ratio.js"></script>

    <!-- sidebar effect -->
    <script src="/assets/js/sidebareffect.js"></script>

    <!-- Theme js -->
    <script src="/assets/js/script.js"></script>
</body>

</html>